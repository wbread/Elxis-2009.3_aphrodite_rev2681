	</files>
	<params>
		<param name="moduleclass_sfx" type="text" dir="ltr" default="" label="AX_SM_MCSL" description="AX_SM_MCSD" />
		<param name="profstyle" type="list" dir="rtl" default="compact" label="AX_SM_NWF_STL" description="AX_DISPSTYLE">
			<option value="compact">AX_COMPACT</option>
			<option value="extended">AX_EXTENDED</option>
		</param>
		<param name="numshow" type="list" default="2" label="AX_SM_NWF_NOIL" description="AX_SM_NWF_NOID">

		<param name="showgender" type="radio" default="0" label="AX_GENDER" description="AX_GENDERDESC">
			<option value="0">AX_NO</option>
			<option value="1">AX_YES</option>
		</param>
		<param name="showage" type="radio" default="0" label="AX_AGE" description="AX_AGEDESC">
			<option value="0">AX_NO</option>
			<option value="1">AX_YES</option>
		</param>
		<param name="showrealname" type="radio" default="0" label="AX_SM_LF_NUNL" description="AX_REALUNAME">
